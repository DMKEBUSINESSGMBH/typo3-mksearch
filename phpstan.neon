    		-
    			message: "#^Instantiated class Zend_Search_Lucene_Search_Query_MultiTerm not found\\.$#"
    			count: 1
    			path: service/engine/class.tx_mksearch_service_engine_ZendLucene.php

    		-
    			message: "#^Instantiated class Zend_Search_Lucene_Search_Query_Phrase not found\\.$#"
    			count: 1
    			path: service/engine/class.tx_mksearch_service_engine_ZendLucene.php

    		-
    			message: "#^Parameter \\$doc of method tx_mksearch_service_engine_ZendLucene\\:\\:addFieldToIndexDoc\\(\\) has invalid type Zend_Search_Lucene_Document\\.$#"
    			count: 1
    			path: service/engine/class.tx_mksearch_service_engine_ZendLucene.php
